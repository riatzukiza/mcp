#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/err/devel/promethean/node_modules/.pnpm/@stryker-mutator+core@8.7.1/node_modules/@stryker-mutator/core/bin/node_modules:/home/err/devel/promethean/node_modules/.pnpm/@stryker-mutator+core@8.7.1/node_modules/@stryker-mutator/core/node_modules:/home/err/devel/promethean/node_modules/.pnpm/@stryker-mutator+core@8.7.1/node_modules/@stryker-mutator/node_modules:/home/err/devel/promethean/node_modules/.pnpm/@stryker-mutator+core@8.7.1/node_modules:/home/err/devel/promethean/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/err/devel/promethean/node_modules/.pnpm/@stryker-mutator+core@8.7.1/node_modules/@stryker-mutator/core/bin/node_modules:/home/err/devel/promethean/node_modules/.pnpm/@stryker-mutator+core@8.7.1/node_modules/@stryker-mutator/core/node_modules:/home/err/devel/promethean/node_modules/.pnpm/@stryker-mutator+core@8.7.1/node_modules/@stryker-mutator/node_modules:/home/err/devel/promethean/node_modules/.pnpm/@stryker-mutator+core@8.7.1/node_modules:/home/err/devel/promethean/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@stryker-mutator/core/bin/stryker.js" "$@"
else
  exec node  "$basedir/../@stryker-mutator/core/bin/stryker.js" "$@"
fi
